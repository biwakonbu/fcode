{
  "globals": {
    "numIndentationSpaces": 4
  },
  "rules": {
    "redundantNewKeyword": {
      "enabled": false
    },
    "functionReimplementation": {
      "enabled": true
    },
    "canBeReplacedWithComposition": {
      "enabled": true
    },
    "reimplementsFunction": {
      "enabled": true
    },
    "uselessBinding": {
      "enabled": true
    },
    "favourIgnoreOverLetWild": {
      "enabled": true
    },
    "wildcardNamedWithAsPattern": {
      "enabled": true
    },
    "unusedValue": {
      "enabled": true
    },
    "unusedOpenStatement": {
      "enabled": true
    },
    "suggestions": {
      "enabled": true
    },
    "numberOfItemsInTuple": {
      "enabled": true,
      "config": {
        "maxItems": 4
      }
    },
    "functionLength": {
      "enabled": true,
      "config": {
        "maxLines": 80
      }
    },
    "memberNamesNeedNotBeginWithCaps": {
      "enabled": true
    },
    "publicValuesNamesNeedNotBeginWithCaps": {
      "enabled": true
    },
    "nonPublicValuesNamesNeedNotBeginWithCaps": {
      "enabled": true
    },
    "exceptionNamesNeedNotBeginWithCaps": {
      "enabled": true
    },
    "typeNamesNeedNotBeginWithCaps": {
      "enabled": true
    },
    "interfaceNamesNeedNotBeginWithCaps": {
      "enabled": true
    },
    "unionCasesNamesNeedNotBeginWithCaps": {
      "enabled": true
    },
    "moduleNamesNeedNotBeginWithCaps": {
      "enabled": true
    },
    "literalNamesNeedNotBeginWithCaps": {
      "enabled": true
    },
    "namespaceNamesNeedNotBeginWithCaps": {
      "enabled": true
    },
    "parametersNeedNotBeginWithCaps": {
      "enabled": true
    },
    "cyclomaticComplexity": {
      "enabled": true,
      "config": {
        "maxComplexity": 15
      }
    },
    "maxNumberOfItemsInTuple": {
      "enabled": true,
      "config": {
        "maxItems": 4
      }
    },
    "maxNumberOfFunctionParameters": {
      "enabled": true,
      "config": {
        "maxItems": 5
      }
    },
    "maxNumberOfMembers": {
      "enabled": true,
      "config": {
        "maxItems": 32
      }
    },
    "maxNumberOfBooleanOperatorsInCondition": {
      "enabled": true,
      "config": {
        "maxItems": 4
      }
    },
    "favourReRaise": {
      "enabled": true
    },
    "raiseWithTooManyArguments": {
      "enabled": true
    },
    "failwithWithSingleArgument": {
      "enabled": true
    },
    "invalidOpWithSingleArgument": {
      "enabled": true
    },
    "nullArgWithSingleArgument": {
      "enabled": true
    },
    "indentationUniquelyEnoughToFormALevel": {
      "enabled": true,
      "config": {
        "mustStartFromFirstColumn": false
      }
    }
  },
  "ignoreFiles": [
    "**/obj/**",
    "**/bin/**",
    "**/.git/**",
    "**/AssemblyInfo.fs"
  ]
}